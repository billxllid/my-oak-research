{
  "name": "my-oak-research",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "turbo": "^2.6.1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.5.1"
}
